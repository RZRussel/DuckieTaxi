formula max_x = @max_x@;
formula max_y = @max_y@;
formula init_x = @init_x@;
formula init_y = @init_y@;
formula init_direction = @init_direction@;
formula max_direction = @max_direction@;

module Taxi
	x: [0..max_x];
	y: [0..max_y];
	direction: [0..max_direction];

	@move@

endmodule

init
    x = init_x & y = init_y & direction = init_direction
endinit
