formula max_x = @max_x@;
formula max_y = @max_y@;
formula init_x = @init_x@;
formula init_y = @init_y@;
formula init_direction = @init_direction@;
formula max_direction = @max_direction@;

module Taxi
	v1: [0..max_x];
	v2: [0..max_y];
	v3: [0..max_direction];

	@move@

endmodule

init
    v1 = init_x & v2 = init_y & v3 = init_direction
endinit
